{
  "name": "podcastdx-client",
  "version": "5.1.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/RyanHirsch/podcastdx-client"
  },
  "scripts": {
    "prettier:base": "prettier --parser typescript",
    "prettier:check": "yarn prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "yarn prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "start": "node dist/index.js",
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint --ext .js,.ts src/",
    "test:watch": "jest --watch",
    "dev": "cross-env DEBUG=* ts-node -r tsconfig-paths/register example.ts",
    "dev:watch": "cross-env DEBUG=* ts-node-dev --respawn --no-notify example.ts",
    "test:episodes": "cross-env DEBUG=* jest episodes",
    "test:podcasts": "cross-env DEBUG=* jest podcasts",
    "test:recent": "cross-env DEBUG=* jest recent",
    "test:search": "cross-env DEBUG=* jest search",
    "validate": "cross-env DEBUG=* ts-node -r tsconfig-paths/register src/schemas/validate.ts"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.10",
    "@types/node-fetch": "^2.5.8",
    "@types/ramda": "^0.31.1",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "auto-changelog": "^2.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-sonarjs": "^3.0.5",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "mixpanel": "^0.13.0",
    "node-fetch": "^2.6.1",
    "ramda": "^0.28.0",
    "typescript-json-schema": "^0.63.0"
  }
}
